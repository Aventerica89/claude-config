        "_alignItems": "center",
        "_background": { "color": { "raw": "var(--primary-ultra-light)" } },
        "_border": {
          "radius": { "top": "var(--radius)", "right": "var(--radius)", "bottom": "var(--radius)", "left": "var(--radius)" }
        },
        "_marginBottom": "var(--space-xs)"
      }
    },
