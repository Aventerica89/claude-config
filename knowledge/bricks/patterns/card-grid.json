      "settings": {
        "_background": { "color": { "raw": "var(--white)" } },
        "_padding": {
          "top": "var(--space-l)",
          "right": "var(--space-l)",
          "bottom": "var(--space-l)",
          "left": "var(--space-l)"
        },
        "_border": {
          "radius": { "top": "var(--radius)", "right": "var(--radius)", "bottom": "var(--radius)", "left": "var(--radius)" }
        },
        "_boxShadow": {
          "offsetX": "0",
          "offsetY": "4px",
          "blur": "20px",
          "color": { "raw": "var(--base-trans-10)" }
        },
        "_display": "flex",
        "_direction": "column",
        "_rowGap": "var(--space-s)",
        "_cssCustom": ".jb-card {\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\n}\n\n.jb-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 12px 40px var(--base-trans-15);\n}"
      }
    },
