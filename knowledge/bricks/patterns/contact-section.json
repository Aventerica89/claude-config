  "version": "2.1.4",
  "globalClasses": [
    {
      "id": "jbct01",
      "name": "jb-contact__eyebrow",
      "settings": {
        "_typography": {
          "font-size": "var(--text-s)",
          "font-weight": "600",
          "text-transform": "uppercase",
          "letter-spacing": "2px",
          "color": { "raw": "var(--primary)" }
        }
      }
    },
    {
      "id": "jbct02",
      "name": "jb-contact__title",
      "settings": {
        "_typography": {
          "font-size": "var(--h2)",
          "font-weight": "700",
          "color": { "raw": "var(--white)" }
        }
      }
    },
    {
      "id": "jbct03",
      "name": "jb-contact__description",
      "settings": {
        "_typography": {
          "font-size": "var(--text-m)",
          "line-height": "1.6",
          "color": { "raw": "var(--neutral)" }
        }
      }
    },
    {
      "id": "jbct04",
      "name": "jb-contact__item",
      "settings": {
        "_display": "flex",
        "_direction": "row",
        "_columnGap": "var(--space-m)",
        "_alignItems": "flex-start"
      }
    },
    {
      "id": "jbct05",
      "name": "jb-contact__label",
      "settings": {
        "_typography": {
          "font-size": "var(--text-xs)",
          "font-weight": "500",
          "text-transform": "uppercase",
          "letter-spacing": "1px",
          "color": { "raw": "var(--neutral)" }
        }
      }
    },
    {
      "id": "jbct06",
      "name": "jb-contact__value",
      "settings": {
        "_typography": {
          "font-size": "var(--text-m)",
          "color": { "raw": "var(--white)" }
        },
        "_cssCustom": ".jb-contact__value:hover {\n  color: var(--primary);\n}"
      }
    },
    {
      "id": "jbct07",
      "name": "jb-contact__social",
      "settings": {
        "iconColor": { "raw": "var(--neutral)" },
        "_cssCustom": ".jb-contact__social {\n  transition: color 0.2s ease;\n}\n\n.jb-contact__social:hover {\n  color: var(--primary);\n}"
      }
    }
  ]
